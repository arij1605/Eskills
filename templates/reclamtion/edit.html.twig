{% extends 'front.html.twig' %}

{% block title %}New Reclamtion{% endblock %}


{% block body %}

    <div class="card-body">

        <h4 class="card-title">Add Reclamation</h4>
        {{ form_start(form, {'attr': {'class': 'forms-sample'}}) }}
        <div class="form-group">
            <label for="exampleInputUsername1">Sujet</label>
            {{ form_widget(form.sujet, { 'attr': {'class': 'form-control'} }) }}
            {{ form_errors(form.sujet) }}
        </div>
        <div class="form-group">
            <label for="exampleInputEmail1">Objet</label>
            {{ form_widget(form.objet, { 'attr': {'class': 'form-control'} }) }}
            {{ form_errors(form.objet) }}

        </div>

        <input type="submit" value="Enregistrer" class="btn btn-success"  formnovalidate/>


        {{ form_end(form) }}
    </div>

{% endblock %}

