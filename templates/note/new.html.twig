{% extends 'FormateurFront.html.twig' %}

{% block title %}New Note{% endblock %}

{% block body %}
    <div class="card-body">

        <h4 class="card-title">Affecter Note</h4>
        {{ form_start(form, {'attr': {'class': 'forms-sample'}}) }}
        <div class="form-group">
            <label for="exampleInputUsername1">Note</label>
            {{ form_widget(form.note, { 'attr': {'class': 'form-control'} }) }}
            {{ form_errors(form.note) }}
        </div>
        <div class="form-group">
            <label for="exampleInputUsername1">Module</label>
            {{ form_widget(form.idExamen, { 'attr': {'class': 'form-control'} }) }}
        </div>

        <input type="submit" value="Enregistrer" class="btn btn-success"/>


        {{ form_end(form) }}
    </div>
{% endblock %}
